{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=check1st;Username=cysun;Password=abcd"
  },
  "Minio": {
    "Endpoint": "sfo3.digitaloceanspaces.com",
    "AccessKey": "abcd",
    "SecretKey": "1234",
    "Bucket": "test",
    "PathPrefix": "",
    "AttachmentTypes": [ ".zip" ],
    "TextTypes": [ ".java", ".sql", ".jsp", ".txt", ".php" ]
